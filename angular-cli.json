{
  "project": {
    "version": "1.0.0-beta.21",
    "name": "FEUPTalks"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "assets/styles/themify-icons.css",
        "assets/styles/materialize.css",
        "assets/styles/spinner.css",
        "assets/styles/main.css",
        "../node_modules/angular2-data-table/release/datatable.css",
        "../node_modules/angular2-data-table/release/assets/icons.css",
        "../node_modules/angular2-data-table/release/material.css",
        "../node_modules/fullcalendar/dist/fullcalendar.min.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "assets/js/jquery-knob.min.js",
        "../node_modules/moment/moment.js",
        "../node_modules/fullcalendar/dist/fullcalendar.min.js",
        "assets/js/materialize.js",
        "assets/js/material-datetime.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
